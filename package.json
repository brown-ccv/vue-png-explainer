{
  "name": "@brown-ccv/vue-png-explainer",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Mary McGrath",
    "email": "mary_mcgrath@brown.edu"
  },
  "main": "dist/vue-png-explainer.umd.js",
  "module": "dist/vue-png-explainer.esm.js",
  "unpkg": "dist/vue-png-explainer.min.js",
  "browser": {
    "./sfc": "src/components/Explainer.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint": "vue-cli-service lint",
    "release": "semantic-release --branches main"
  },
  "dependencies": {
    "bulma": "^0.9.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/compiler-sfc": "^3.0.0-rc.2",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "rollup": "^2.22.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^6.0.0-beta.9",
    "sass-loader": "^9.0.2",
    "semantic-release": "^17.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  }
}

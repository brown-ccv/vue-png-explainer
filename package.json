{
  "name": "@brown-ccv/vue-png-explainer",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "author": {
    "name": "Mary McGrath",
    "email": "mary_mcgrath@brown.edu"
  },
  "main": "dist/vue-png-explainer.umd.js",
  "module": "dist/vue-png-explainer.esm.js",
  "unpkg": "dist/vue-png-explainer.min.js",
  "browser": {
    "./sfc": "src/components/Explainer.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-png-explainer.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-png-explainer.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-png-explainer.min.js",
    "lint": "vue-cli-service lint",
    "release": "semantic-release --branches main"
  },
  "dependencies": {
    "bulma": "^0.9.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/compiler-sfc": "^3.0.0-rc.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "rollup": "^2.22.1",
    "rollup-plugin-vue": "^6.0.0-beta.9",
    "sass-loader": "^9.0.2",
    "semantic-release": "^17.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
